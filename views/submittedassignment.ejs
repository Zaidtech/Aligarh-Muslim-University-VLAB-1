<%- include ('./partials/header') %>
	<div>
		<div>
			<% if(currentUser.type=="teacher" ){ %>
				<% items.forEach(function(assignment) { %>
                    <div class="w3-container w3-card w3-white w3-round w3-margin">
                        <br />
        
                        <hr class="w3-clear" />
                        <h4 style="color: red">
                            <%= assignment.name %>(<%= assignment.enrollment_no %>)
                        </h4>
                       
                        <p style="color: green">Submitted on :-<%= assignment.created %>
                        </p>
                        <div class="w3-row-padding" style="margin: 0 -16px">
                            <div class="w3-half">
                                <td>
                                    <embed src="<%= assignment.img %>" width="100px" height="100px" />
                                   
                                </td>
                            </div>
                        </div>
                
                        <div></div>
                    </div>
                    <% }) %>
			<%} else {%>
		</div>
		<% items.forEach(function(assignment) { %>
			  
            <% if(currentUser.username == assignment.name){ %>
                <div class="w3-container w3-card w3-white w3-round w3-margin">
                    <br />
    
                    <hr class="w3-clear" />
                    <h4 style="color: red">
                        <%= assignment.name %>(<%= assignment.enrollment_no %>)
                    </h4>
                   
                    <p style="color: green">Submitted on :-<%= assignment.created %>
                    </p>
                    <div class="w3-row-padding" style="margin: 0 -16px">
                        <div class="w3-half">
                            <td>
                                <embed src="<%= assignment.img %>" width="100px" height="100px" />
                               
                            </td>
                        </div>
                    </div>
            
                    <div></div>
                </div> 
            <% } else{ %>


                <% } %>

           




			<% }) %>
		</div>
	<%}%>
	<%- include ('./partials/footer') %>